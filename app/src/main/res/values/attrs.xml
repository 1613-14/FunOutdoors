<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义分割线加文本的控件的属性-->
    <declare-styleable name="SplitLineView">
        <attr name="textContent" format="string" />
        <attr name="lineColor" format="color" />
    </declare-styleable>
    <!--自定义已登录后的导航条目的控件的属性-->
    <declare-styleable name="LoginedBannerItem">
        <attr name="bannerTitle" format="string" />
    </declare-styleable>
    <!--自定义的身份证添加图片的控件的属性-->
    <declare-styleable name="IDCartView">
        <attr name="bgImage" format="reference" />
    </declare-styleable>
    <!--自定义的消息通知的选择item-->
    <declare-styleable name="MessageItem">
        <attr name="messageItemIcon" format="reference" />
        <attr name="messageItemText" format="string" />
    </declare-styleable>
    <!--自定义个人资料的每一个item-->
    <declare-styleable name="PersonInfoItem">
        <attr name="leftTitle" format="string" />
    </declare-styleable>
    <!-- 游记日期安排列表-->
    <declare-styleable name="GuideDetailItem">
        <attr name="dataTitle" format="string" />
    </declare-styleable>
</resources>