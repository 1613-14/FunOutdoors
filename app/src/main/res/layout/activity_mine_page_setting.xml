<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grey_white"
    android:orientation="vertical"
    tools:context="com.phone.funoutdoors.activity.MinePage_SettingActivity">

    <include layout="@layout/toolbar_item" />

    <TextView
        android:id="@+id/offLineMap"
        style="@style/SettingButtonStyle"
        android:text="离线地图" />

    <TextView
        android:id="@+id/safe"
        style="@style/SettingButtonStyle"
        android:text="账号安全" />

    <TextView
        android:id="@+id/about"
        style="@style/SettingButtonStyle"
        android:text="关于" />

    <TextView
        android:id="@+id/clearCache"
        style="@style/SettingButtonStyle"
        android:text="清空缓存" />

    <TextView
        android:id="@+id/exitLogin"
        style="@style/SettingButtonStyle"
        android:gravity="center"
        android:text="退出登录"
        android:textColor="@color/orangle" />
</LinearLayout>
